%h1= t('travels.new.h1')
- form_for @travel, :url => travels_path, :builder => OlliFormBuilder do |f|
  = f.error_messages
  %ol.form
    = f.text_field :event
    = f.text_field :url
    = f.text_field :location
    = f.date_select :from
    = f.date_select :to
  = f.submit t('travels.new.button')
